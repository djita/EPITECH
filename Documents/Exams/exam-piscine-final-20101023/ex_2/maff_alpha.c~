int	my_putchar(char c)
{
  write(1, &c, 1);
}

int	maff_alpha()
{
  char	alpha;

  alpha = 97;
  while (alpha >= 97 && alpha < 122)
    {
      my_putchar(alpha);
      alpha = alpha - 31;
      if (alpha > 65 && alpha <= 90)
	{
	  my_putchar(alpha);
	  alpha = alpha + 33;
	}
    }
  my_putchar(10);
  return;
}

int	main()
{
  maff_alpha();
}
